- if !grades.empty?
  %table.table.table-striped
    %thead
      %tr
        %th{:width => '15%'} Date 
        %th{:width => '10%'} Score
        %th{:width => '15%'} Time
        %th{:width => '25%'} Notes
        %th{:width => '15%'} User
    %tbody
      - grades.each do |grade|
        %tr 
          %td= grade.due_date.strftime("%m/%d/%y") rescue ""
          %td= ::Util.round_f(grade.accuracy).to_s+"%" rescue ""
          %td= grade.time_to_complete.strftime("%H:%M") rescue ""
          %td
            .wordbreak
              = grade.note
          %td= grade.user.full_name
#grades_pages= render_pagination grades

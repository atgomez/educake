- content_for :js do
  = javascript_include_tag('app/students.js')
  
.teacher
  .clearfix.toolbar
    - unless @series.empty?
      = link_to "View chart", students_path(:user_id => @user.id),  :class => "brown font18"
    - else
      = link_to "Back", students_path(:user_id => @user.id),  :class => "brown font18"
  = render('shared/students/list', :with_pagination => false, :students => @students, :user => @user)
